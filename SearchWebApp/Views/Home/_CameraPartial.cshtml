@model IEnumerable<Camera>

@foreach (var camera in Model)
{
    <tr>
        <td>@camera.Id</td>
        <td>@camera.Name</td>
        <td>@camera.Latitude</td>
        <td>@camera.Longitude</td>
    </tr>
}